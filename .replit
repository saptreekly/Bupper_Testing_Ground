modules = ["python-3.11", "python3"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "QAOA Routing Optimizer"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "QAOA Test"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "QAOA Tests"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "QAOA Benchmark"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "QAOA Street Network"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flask Server"

[[workflows.workflow]]
name = "QAOA Routing Optimizer"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python example.py"

[[workflows.workflow]]
name = "QAOA Test"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python example.py --backend qiskit --cities 4 --hybrid --qaoa-depth 2"

[[workflows.workflow]]
name = "QAOA Tests"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python -m unittest test_qaoa.py -v"

[[workflows.workflow]]
name = "QAOA Benchmark"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python example.py --benchmark --grid-size 8"

[[workflows.workflow]]
name = "QAOA Street Network"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python example.py --cities 4 --location 'San Francisco, California, USA' --backend qiskit --hybrid"

[[workflows.workflow]]
name = "Flask Server"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python app.py"
waitForPort = 3000

[deployment]
run = ["sh", "-c", "python example.py --backend qiskit --cities 4 --hybrid --qaoa-depth 2"]

[[ports]]
localPort = 3000
externalPort = 80

[[ports]]
localPort = 4000
externalPort = 3000

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 8080
externalPort = 8080
